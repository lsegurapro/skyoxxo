{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build api",
    "build:consumer": "nest build consumer",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "PROCONTACTO_ENV=local nest start api",
    "start:local": "PROCONTACTO_ENV=local nest start api --debug 0.0.0.0:9662 --watch",
    "start:prod": "node dist/apps/api/main.js",
    "start:consumer": "PROCONTACTO_ENV=local nest start consumer",
    "start:local:consumer": "PROCONTACTO_ENV=local nest start consumer --watch",
    "start:prod:consumer": "node dist/apps/consumer/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:silent": "jest --coverage --silent",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/project-name/test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:local": "PROCONTACTO_ENV=local node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^1.16.2",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.17",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "@procontacto/cls-rtracer": "file:../cls-rtracer/",
    "@procontacto/commons-logger": "file:../commons-logger/",
    "@types/webpack-env": "^1.16.0",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.7.1",
    "axios": "^0.21.1",
    "bull": "^3.22.6",
    "cache-manager": "^3.4.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express-xml-bodyparser": "^0.3.0",
    "helmet": "^4.6.0",
    "mariadb": "^2.5.3",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-model-generator": "^0.4.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/amqp-connection-manager": "^2.0.10",
    "@types/bull": "^3.15.1",
    "@types/cache-manager": "^3.4.0",
    "@types/cache-manager-redis-store": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.38.1",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests/"
    ]
  }
}
